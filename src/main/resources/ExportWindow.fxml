<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="312.0" prefWidth="508.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ExportWindowController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="305.0" prefWidth="640.0" VBox.vgrow="ALWAYS">
      <children>
            <Label fx:id="directoryLabel" alignment="CENTER" layoutX="82.0" layoutY="103.0" prefHeight="17.0" prefWidth="359.0" textAlignment="CENTER" />
            <Button layoutX="201.0" layoutY="56.0" mnemonicParsing="false" onAction="#directoryDialog" text="Choose directory" />
            <CheckBox fx:id="mergeCheckBox" layoutX="200.0" layoutY="182.0" mnemonicParsing="false" text="Merge all results" />
            <Button layoutX="225.0" layoutY="247.0" mnemonicParsing="false" text="Proceed" onAction="#proceedAction"/>
      </children>
    </AnchorPane>
  </children>
</VBox>
