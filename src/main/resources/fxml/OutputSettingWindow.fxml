<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="203.0" prefWidth="347.0" xmlns="http://javafx.com/javafx/15" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="cz.muni.fi.tovarys.controller.fxml.OutputSettingController">
    <AnchorPane prefHeight="226.0" prefWidth="347.0" VBox.vgrow="ALWAYS">
        <TextField fx:id="inputCols" layoutX="130.0" layoutY="58.0" prefHeight="31.0" prefWidth="114.0"
                   promptText="unlimited if empty" text="10"/>
        <Label layoutX="130.0" layoutY="9.0" prefHeight="46.0" prefWidth="207.0"
               text="Note: It is not recommended to put high values for columns." wrapText="true"/>
        <TextField fx:id="inputRows" layoutX="130.0" layoutY="103.0" prefHeight="31.0" prefWidth="114.0"
                   promptText="unlimited if empty"/>
        <Label layoutX="35.0" layoutY="65.0" prefHeight="17.0" prefWidth="61.0" text="Columns:"/>
        <Label layoutX="35.0" layoutY="110.0" prefHeight="17.0" prefWidth="61.0" text="Rows:"/>
        <Button layoutX="143.0" layoutY="164.0" mnemonicParsing="false" onAction="#save" prefHeight="25.0"
                prefWidth="61.0" text="Save"/>
    </AnchorPane>
</VBox>
