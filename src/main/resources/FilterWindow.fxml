<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.FilterWindowController">
    <children>
        <ListView fx:id="listViewAvailable" layoutX="31.0" layoutY="98.0" prefHeight="120.0" prefWidth="168.0" />
        <ListView fx:id="listViewUsed" layoutX="301.0" layoutY="98.0" onMousePressed="#constraintWindowAction" prefHeight="120.0" prefWidth="168.0" />
        <Label layoutX="91.0" layoutY="72.0" text="Available" />
        <Label layoutX="372.0" layoutY="72.0" text="Used" />
        <Button layoutX="234.0" layoutY="125.0" mnemonicParsing="false" onAction="#addFilter" text="&gt;&gt;" />
        <Button layoutX="234.0" layoutY="158.0" mnemonicParsing="false" onAction="#removeFilter" text="&lt;&lt;" />
        <Button fx:id="applyButton" layoutX="227.0" layoutY="251.0" mnemonicParsing="false" text="Apply" />
    </children>
</AnchorPane>
